.search-wrap .search-input .btn-camera .ion-ios-search{color:#00a9e9;font-size:2em;}
.search-wrap .search-input .btn-camera .ion-ios-close{color:#7e7e7e;font-size:1.6em;background-image:url(//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/close.png);right:0;}
.search-wrap .search-input .btn-camera .ion-qr-scanner{color:#7e7e7e;font-size:1.6em;background-image:url(//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/scan.png);}
.search-wrap .search-input .btn-camera span{color:#7e7e7e;font-size:1.6em;}
