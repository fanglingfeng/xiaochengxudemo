.state-collect + .state-collect {margin-left:10px;}
.state-apply {color:#77d400;border:1px solid #77d400;}
.state-order {color:#00a9e9;border:1px solid #00a9e9;}

.list-hd {padding:.5em .4em;color:#333;font-size:1.1em;font-weight:normal;display:block;}

.list-hd .hd-name {border-left:4px solid #00a9e9;padding-left:10px;line-height:1.5;}

.big-icon {position:relative;vertical-align:middle;top:0px;}
.big-icon::before {content:"";display:inline-block;width:1.5em;}
.big-icon i {position:absolute;left:0em;top:50%;font-size:1.5em;transform:translateY(-50%);-webkit-transform:translateY(-50%);width:100%;height:100%;display:block;}
.big-icon i .icon-img {vertical-align:top;}

.item-actions {display:flex;justify-content:space-around;background:#f8f8f8;font-size:.8em;}
