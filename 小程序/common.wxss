/*办事指南*/
.menu-list .nav-menu{min-width:6em;height:100%;position:fixed;left:0;top:0;box-shadow:0 3px 5px #7e7e7e;background-color:#fff;z-index:9;}
.menu-list .nav-menu .title{text-align:center;height:0.8em;line-height:0.8em;border-bottom:1px solid #cccccc;}
.menu-list .nav-menu .title:hover,.menu-list .nav-menu .title.hover{color:#fff;background-color:#1885d3;}
.menu-list .nav-list{margin-left:6em;background-color:#fff;}
.menu-list .nav-list .item{padding-right:.2em;position:relative;}
.menu-list .nav-list .item .v-center{right:.2em;color:#cccccc;}
.m-weui-tab{padding-left:0;padding-right:0;padding-bottom:0;}
.m-weui-tab .weui-form-preview__item{overflow:auto;}
.m-weui-tab::after{left:0;border:none;}
.m-weui-tab .weui-form-preview__label{margin-right:0;width:25%;min-width:unset;text-align:center;text-align-last:center;position:relative;padding-bottom:10px;color:#000;}
.m-weui-tab .weui-form-preview__label::after{content:" ";position:absolute;left:0;bottom:0;right:0;height:1px;border-bottom:1px solid #D9D9D9;color:#D9D9D9;-webkit-transform-origin:0 100%;transform-origin:0 100%;-webkit-transform:scaleY(0.5);transform:scaleY(0.5);left:0;}
.m-weui-tab .weui-form-preview__label.hover,.m-weui-tab .weui-form-preview__label:hover,.m-weui-tab .weui-form-preview__label:active{color:#1885d3;}
.m-weui-tab .weui-form-preview__label.hover::after,.m-weui-tab .weui-form-preview__label:hover::after,.m-weui-tab .weui-form-preview__label:active::after{border-color:#1885d3;z-index:3;}

/*新闻*/
.news-list .item{padding:.2em;overflow:hidden;}
.news-list .item .img{width:25%;height:4.5em;margin-right:1em;overflow:hidden;float:left;}
.news-list .item .img img{width:100%;height:100%;vertical-align:middle;}
.news-list .item .text-title{display:-webkit-box;line-height:1.5;overflow:hidden;text-overflow:ellipsis;-webkit-box-orient:vertical;-webkit-line-clamp:2;}
.news-list .item:active{background-color:#f1f1f1;}
.news-detail{height:100%;}
.news-detail .title{color:#1885d3;font-size:1.2em;text-align:center;}
.news-detail .content img{max-width:100%;}
#container{height:100%;}
#container  div{height:100%;}
#list{position:absolute;z-index:1;left:0;top:0;width:100%;height:100%;overflow-y:scroll;}
#detail{height:100%;width:100%;position:absolute;z-index:999;left:0;top:0;background-color:#f1f1f1;display:none;overflow-y:scroll;}
.load-more{height:0.8em;line-height:0.8em;width:100%;background-color:rgba(24,133,211,0.6);color:#fff;text-align:center;font-size:1.2em;}
.load-more.disable{background-color:#7e7e7e;}
.click:active,.click:hover{background-color:#f1f1f1;}
.weui-loadmore_line .weui-loadmore__tips{background-color:#f1f1f1;}


/*我的预约*/
.text-list {}
.text-list .state {position:absolute;right:5px;width:50px;height:62px;top:-5px;background:url("") no-repeat center;background-size:100% 100%;}
.text-list .state .state-text {color:#fff;font-size:1em;line-height: 1;width:2em;text-align:center;margin: auto;position: relative;box-sizing: border-box;transform: translateY(-70%);top: 50%;-webkit-transform: translateY(-70%);top: 50%;}
.text-list .state.state-success {background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/icon/order-success.png');}
.text-list .state.state-failed {background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/icon/order-failed.png');}
.text-list .text-item {border:1px solid #dcdcdc;position:relative;}
.text-list .text-item .hd,.text-list .text-item .bd {display:block;}
.text-list .text-item .hd {width:6em;float:left;}
.text-list .text-item .bd {margin-left:6.5em;white-space:normal;word-break: break-all;}

/*完善个人信息*/
.tab-wrap {overflow:hidden;border-radius:5px;border:1px solid #00a9e9;background-color:#00a9e9;}
.tab-wrap .hd {color:#00a9e9;background-color:#fff;border-radius:5px;display:inline-block;width:50%;text-align:center;font-size:1.5em;padding:5px;box-sizing:border-box;}
.tab-wrap .hd.hover {background-color:#00a9e9;color:#fff;}
.verify-wrap {display:flex;}
.verify-wrap .input-group {flex:1;}
.verify-wrap .btn {width:40%;display:inline-block;padding-left:1em;min-width:10em;}
.verify-wrap .btn .weui-btn {line-height:1.2;padding:11px 0;}
.line-m {border-bottom:1px solid #dcdcdc;margin:20px -.4em;}
.line-m-pure {border-bottom:1px solid #dcdcdc;}

.verify-wrap .search-btn {width:30%;display:inline-block;padding-left:1em;min-width:3em;}

.avatar {margin:30px auto 0;width:6em;heigt:6em;overflow:hidden;border-radius:100%;}
.avatar img {width:100%;height:100%;}

/*我的咨询*/
.my-talk-list .weui-cell {padding:16px 15px; }
.my-talk-list.big-margin .weui-cell {padding: 1em 15px;}
.state-reply {border-radius:3px;width:5em;text-align:center;display:inline-block;padding:5px 0;}
.state-reply {margin-left:10px;}

.talk-from,.talk-to {position:relative;padding:10px;border-radius:3px;display:inline-block;}
.talk-from::after,.talk-to::after {content:"";display:block;clear:both;}
.talk-from {border:1px solid #dcdcdc;background-color:#fff;margin-top:10px;}
.talk-to {margin-bottom:10px;background-color:#00a9e9;float:right;color:#fff;}

.arrow-up,.arrow-down {display:block;position:absolute;width:10px;height:10px;transform:rotate(45deg);-webkit-transform:rotate(45deg);}
.arrow-up {left:10px;top:-5px;border:1px solid #dcdcdc;border-color:#dcdcdc transparent transparent #dcdcdc;background-color:#fff;}
.arrow-down {right:10px;bottom:-5px;background-color:#00a9e9}

.link-list .weui-cells {font-size:1em;}
.body-f-size {font-size:0.8em;}
