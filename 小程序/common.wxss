.step-time {flex:1;position:relative;padding:10px;border-radius:5px;color:#666;margin-left:15px;}
.step-time::after {content:"";display:block;width:10px;height:10px;position:absolute;top:50%;left:-5px;transform:translateY(-50%) rotate(45deg);-webkit-transform:translateY(-50%) rotate(45deg);background-color:inherit;}
.step-done-wrap {width:6em;}
.step-done {display:inline-block;background:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/icon/step-done.png') no-repeat center;width:14px;height:42px;background-size:100%;}
.step-done .img {height:42px;width:14px;}
.step-doing {background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/icon/step-doing.png');}

/*办事流程*/
.rout-item {width:303px;margin:auto;padding-top:20px;}
.rout-item .rout {float:left;width:150px;height:61px;background:url('') no-repeat center;background-size:100%;}
.rout.rout-r {padding-left:35px;background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/rout-r.png');position:relative;left:-9px;}
.rout.rout-r strong {margin:11px 0 0 33px;display:block;}
.rout.rout-r p {text-align:center;}
.rout p {padding-top:2px;}
.rout.rout-l {padding-right:35px;background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/rout-l.png');position:relative;right:-9px;z-index:1;}
.rout.rout-l strong {display:block;margin:11px 31px 0 auto;text-align:right;}
.rout.rout-l p {text-align:center;}
.rout.rout-r-hover {background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/rout-r-hover.png');color:#fff;left:-6px;}
.rout.rout-l-hover {background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/rout-l-hover.png');color:#fff;}
.rout.rout-l-hover ,.rout.rout-r-hover {color:inherit;}


.rout-line {height:75px;width:3px;background:#ccc;position:relative;margin-top:-28px;float:left;}


/*选择办理事项*/
.tab-hd {list-style:none;}
.tab-hd li {float:left;padding:10px 0;text-align:center;font-size:1.1em;line-height:1.2;width:50%;}
.tab-hd li.hover {color:#00a9e9;border-bottom:3px solid #00a9e9;}
.theme-wrap ul {list-style:none;}
.theme-wrap li {float:left;width:50%;padding:5px 5px;border:1px solid #dcdcdc;border-width:0 1px 1px 0;display:flex;align-items:center;}
.theme-wrap li:nth-child(2n) {border-right:none;}
.theme-wrap li:nth-child(1),.theme-wrap li:nth-child(2) {border-top-width:1px;}
.theme-wrap li i {display:block;width:35%;height:22px;float:left;text-align:right;margin-right:10px;}
.theme-wrap li i img {height:100%;width:auto;}
.theme-wrap li h4 {font-weight:normal;font-size:1.2em;}

.theme-wrap-s li i {height:22px;}

.tab-hd .li {float:left;padding:10px 0;text-align:center;font-size:1.1em;line-height:1.2;width:50%;box-sizing:border-box;-webkit-box-sizing:border-box;}
.tab-hd .li.hover {color:#00a9e9;border-bottom:3px solid #00a9e9;}
.theme-wrap .li {float:left;width:50%;padding:12px 5px;border:1px solid #dcdcdc;border-width:0 1px 1px 0;display:flex;align-items:center;-webkit-box-sizing:border-box;}
.theme-wrap .li:nth-child(2n) {border-right:none;}
.theme-wrap .li:nth-child(1),.theme-wrap .li:nth-child(2) {border-top-width:1px;}
.theme-wrap .li .i {display:block;width:30%;height:22px;float:left;text-align:right;margin-right:15px;}
.theme-wrap .li .i .img {height:22px;width:22px;}
.theme-wrap .li .h4 {font-weight:normal;font-size:1.2em;}

.theme-wrap-s li i {height:22px;}


.btn-wrap {display:flex;display:-webkit-flex;align-items:center;justify-content:space-around;padding:10px;}
.btn-wrap .btn-item {border-radius:5px;text-align:center;padding:0px 5px;color:#fff;font-size:1.3em;flex:1;margin:3px;line-height:2;}

.hd-rout {display:flex;display:-webkit-flex;align-items:center;justify-content:space-around;font-size:1.2em;padding:0 .2rem;text-align:center;}
.hd-rout .top-ic {display:block;width:1em;height:1em;background:url('') no-repeat center;background-size:100%;position:relative;top:3px;margin:0 auto;}
.hd-rout .icon-info-gray {background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/icon/icon-info-gray.png');}
.hd-rout .ion-checkmark-circled {background-image:url('//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/icon/ion-checkmark-circled.png');}
.hd-rout i {font-size:1em;visibility:hidden;}
.hd-rout .text {padding:.1rem 0;border-bottom:3px solid #fff;}
.hd-rout .hover {color:#00a9e9;border-bottom:3px solid #00a9e9;}
.hd-rout .rout-circle {background-color:#dcdcdc;color:#fff;border-radius:100%;display: inline-block;width: 12px;height: 12px;text-align: center;line-height: 12px;font-size: 12px;border:2px solid #dcdcdc;box-sizing:content-box;-webkit-box-sizing:content-box;vertical-align: middle;position: relative;top: -2px;}
.hd-rout .hover .rout-circle, .hd-rout .bg-blue.rout-circle {background-color:#00a9e9;border-color:#00a9e9;}
.hd-rout .bg-orange.rout-circle {border-color:#fd6200;}



.img-list {box-sizing:border-box;background-color:rgba(0,0,0,0.5);height:100%;position:fixed;width:100%;overflow:hidden;padding-top:50%;top:0;left:0;bottom:0;right:0;z-index:11;}
.img-list .img-ul {display:block;height:100%;width:100%;background-color:#fff;list-style:none;padding:.2em .2em 5em;overflow-y:auto;margin-bottom:10em;box-sizing:border-box;}
.img-list .img-ul li {height:12em;width:50%;padding:.8em .2em .2em;position:relative;text-align:center;float:left;box-sizing:border-box;}
.img-list .img-ul li .btn-close {position:absolute;top:0;right:.2em;font-size:2em;}
.img-list .img-ul li .btn-close image {width:1em;height:1em;}
.img-list .img-ul li .img {height:100%;overflow:hidden;}
.img-list .img-ul li .img img,
.img-list .img-ul li .img image {width:100%;height:100%;}
.img-list .btn-wrap {position:absolute;bottom:-10px;left:0;right:0;background-color:#fff;}

.list-key-val .weui-cells .weui-cell__bd {min-width:5em;}

/*材料列表 12-23*/
.my-talk-list.material-list.weui-cells {background:#f8f8f8;}
.my-talk-list.material-list.weui-cells:after {border:none;}
.my-talk-list.material-list .weui-cell {padding: 10px 15px;background: #fff;margin-bottom: 10px;box-sizing:border-box;}
.my-talk-list.material-list .weui-cell:before {border:none;}
.material-title-wrap {overflow:hidden;width:calc(100vw - 30px);}
.material-title {line-height:1.5;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.material-title-text {width:calc(100% - .5em);line-height:1.5;display:inline-block;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.marerial-more {width:2em;line-height:1.5;display:none;}

.material-wrap {border-top:1px dashed #dcdcdc;padding-top:.5em;margin-top:5px;width:calc(100vw - 30px);}
.material-wrap .material-state {width:1em;height:2em;line-height:2;}
.material-wrap .material-state i {font-size:1em;width:2em;height:2em;}
.material-wrap .material-add {width:2em;height:2em;line-height:2;border:1px solid #dcdcdc;background:url(//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/icon/jia.png) no-repeat center #f1f1f1;overflow:hidden;position:relative;background-size:100%;box-sizing:border-box;}
.material-wrap .material-add i {font-size:3em;position:absolute;left:50%;top:50%;-webkit-transform:translate3d(-50%,-50%,0);line-height:2;display:block;}
.material-wrap .material-add i .icon-img {left:10%;}
.material-imgs-wrap {margin:0 2.3em 0 1.8em;position:relative;height:2em;overflow:hidden;background:#f8f8f8;}
.material-imgs-wrap .img-loading {right:0;width:2em;height:2em;text-align:center;line-height:2;top:0;}
.material-imgs-wrap .material-imgs-shade {position:absolute;top:0;bottom:0;left:0;right:0;background-color:rgba(0, 169, 233, 0.7);font-size:.8em;line-height:2.5;}
.material-imgs-wrap .material-imgs {overflow:hidden;position:relative;}
.material-imgs-wrap .material-imgs img,
.material-imgs-wrap .material-imgs .material-img {width:1em;height:1.5em;float:left;margin:.25em 2px;}

.btn-jia {width:50px;height:50px;background:url(//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/nav/btn-jia.png) no-repeat center;background-size:100%;float:right;border-radius:100%;position:relative;right:-9px;}
.btn-jia.hover {background-image: url(//weigov.baoan.gov.cn/weigov/plug-in/weixin/weigov/images/nav/btn-close.png);}
.btn-jia-fixed {position:fixed;z-index:14;bottom:1.5em;right:2em;margin-right:-9px;}
.nav-fixed {position:fixed;z-index:13;left:0;right:0;top:0;bottom:0;background-color:rgba(0,0,0,.4);}
.nav-fixed .nav-list-wrap {position:absolute;bottom:1.5em;right:2em;}
.nav-fixed .nav-list .item {display:block;}
.nav-fixed .nav-list .item .text{text-align:center;color:#333;padding: 0.3em 1em;margin: 0 10px 10px;background:#fff;display:inline-block;position: relative;top: -5px;border-radius: 5px;}
.nav-fixed .nav-list .item .img {width:1.5em;height:1.5em;border-radius:100%;background:#fff;padding:0.7em;display:inline-block;}
.nav-fixed .nav-list .item .img image {width:100%;height:100%;}


.animated{-webkit-animation-duration:1s;animation-duration:1s;animation-timing-function:linear;-webkit-animation-timing-function:linear;}
.animated.infinite{-webkit-animation-iteration-count:infinite;animation-iteration-count:infinite;}
.circle{-webkit-animation-name:circle;animation-name:circle;}
@-webkit-keyframes circle{
    0%{-webkit-transform:rotate(0deg);transform:rotate(0deg);}
    100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);}
}


/*收件地址*/
.blue-radio .weui-icon-checked {position:relative;top:-3px;}
.weui-cells_checkbox .no-hover.weui-cell:active {background-color:transparent;}
.position-item {border-bottom:1px solid #ededed;}